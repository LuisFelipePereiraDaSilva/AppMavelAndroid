<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marvelandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ViewListCharacters">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ViewDetailsCharacter"></activity>
        <activity android:name=".activity.ViewDetailsSerie"></activity>
        <activity android:name=".activity.ViewDetailsStorie"></activity>
        <activity android:name=".activity.ViewDetailsComic"></activity>
        <activity android:name=".activity.ViewDetailsCreator"></activity>
        <activity android:name=".activity.ViewDetailsEvent"></activity>
    </application>

</manifest>